<script>
	var a1 = "홍길동";
	var a2 = "웹사이트";
	var a3 = "쇼핑몰";
	var a4 = "회원가입";
	
	var f;
	for(f=1;f<=4;f++){
		var result = eval("a"+f); //eval : 문자 또는 숫자를 이용하여 변수명으로 변환 시키는 함수 
		console.log(result);
	}
</script>
